<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
		"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
		"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="hibernate.connection.driver_class">com.mysql.cj.jdbc.Driver</property>
        <property name="hibernate.connection.url">jdbc:mysql://127.0.0.1:3306/ApartmentFinder</property>
        <property name="hibernate.connection.username">root</property>
        <property name="hibernate.connection.password">Root@1234</property>
        <property name="hibernate.dialect">org.hibernate.dialect.MySQL5Dialect</property>
    	<mapping class="com.finalProject.pojo.RolesModel"/>
    	<mapping class="com.finalProject.pojo.RentList" />
    	<mapping class="com.finalProject.pojo.Messages" />
    	<mapping class="com.finalProject.pojo.AppliedUsers" />
    	<mapping package="com.finalProject.dao"/>
    </session-factory>
</hibernate-configuration>
